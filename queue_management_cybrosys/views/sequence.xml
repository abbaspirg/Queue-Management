<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
      <record id="sequence_form" model="ir.ui.view">
        <field name="name">sequence.form</field>
        <field name="model">sequence</field>
        <field name="arch" type="xml">
            <form>
                <field name="name"/>
            </form>
        </field>
    </record>
      <record id="sequence_tree" model="ir.ui.view">
        <field name="name">sequence.tree</field>
        <field name="model">sequence</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>
     <record id="sequence_action" model="ir.actions.act_window">
        <field name="name">Sequence</field>
        <field name="res_model">sequence</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>