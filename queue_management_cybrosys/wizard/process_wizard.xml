<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="process_wizard_tree" model="ir.ui.view">
        <field name="name">process.wizard.form</field>
        <field name="model">process.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="department_id"/>
                    </group>
                    <footer>
                        <button string="Start Processing" type="object"
                                name="process_start"
                                class="oe_highlight o_onboard_button"/>
                        <button class="o_onboard_button_white"
                                special="cancel"
                                string="Cancel"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>